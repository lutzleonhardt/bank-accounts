{
  "name": "ppe-analyzer",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/main_electron.js",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --extract-css",
    "start:electron:dev": "electron .",
    "build:electron:dev": "webpack --config webpack.electron.js",
    "prod": "npm run build:prod && npm run build:electron:prod && npm run package:prod",
    "build:prod": "ng build --prod --base-href \"../dist/\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:electron": "mocha-webpack --require source-map-support/register --colors --webpack-config webpack.electron.test.js --glob \"*.test.ts\" electron",
    "build:electron:prod": "webpack -p --config webpack.electron.js",
    "package:prod": "electron-packager . bank-accounts --platform=win32 --arch=x64 --overwrite --ignore=\"^((?!node_modules).)*(src|e2e|electron|.idea|.angular-cli.json|.editorconfig|karma.conf.js|protractor.conf.js|README.md|tsconfig.json|tsconfig.electron.json|tslint.json|webpack.electron.js|    .gitignore|.tmp|tsconfig.electron.test.json|webpack.electron.test.js)$\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.2",
    "@angular/cdk": "^5.0.2",
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/forms": "^5.1.2",
    "@angular/http": "^5.1.2",
    "@angular/material": "^5.0.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "@angular/router": "^5.1.2",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "^4.1.1",
    "core-js": "^2.4.1",
    "facebookgraph": "^0.3.0",
    "fb": "^2.0.0",
    "ngrx-store-freeze": "^0.2.0",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.6.2",
    "@angular/compiler-cli": "^5.1.2",
    "@types/facebook-js-sdk": "^2.8.3",
    "@types/jasmine": "2.8.2",
    "@types/node": "~8.5.2",
    "codelyzer": "~4.0.2",
    "electron": "^1.6.10",
    "electron-packager": "^10.1.0",
    "electron-reload": "^1.2.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^4.0.1",
    "mocha-webpack": "^1.0.1",
    "protractor": "~5.2.2",
    "source-map-support": "^0.5.0",
    "ts-loader": "^3.2.0",
    "ts-node": "~4.0.2",
    "tslint": "~5.8.0",
    "typescript": "^2.2.2",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
