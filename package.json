{
  "name": "bank-accounts",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/main_electron.js",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --extract-css",
    "start:electron:dev": "electron .",
    "build:electron:dev": "webpack --config webpack.electron.js",
    "prod": "npm run build:prod && npm run build:electron:prod && npm run package:prod",
    "build:prod": "ng build --prod --base-href \"../dist/\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:electron": "mocha-webpack --require source-map-support/register --growl --colors --webpack-config webpack.electron.test.js --glob \"*.test.ts\" electron",
    "build:electron:prod": "webpack -p --config webpack.electron.js",
    "package:prod": "electron-packager . bank-accounts --platform=win32 --arch=x64 --overwrite --ignore=\"^((?!node_modules).)*(src|e2e|electron|.idea|.angular-cli.json|.editorconfig|karma.conf.js|protractor.conf.js|README.md|tsconfig.json|tsconfig.electron.json|tslint.json|webpack.electron.js|    .gitignore|.tmp|tsconfig.electron.test.json|webpack.electron.test.js)$\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.1.3",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.5",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "axios": "^0.16.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.5",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "electron": "^1.6.10",
    "electron-packager": "^8.7.0",
    "electron-reload": "^1.2.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^3.4.2",
    "mocha-webpack": "^0.7.0",
    "protractor": "~5.1.0",
    "source-map-support": "^0.4.15",
    "ts-loader": "^2.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "^2.2.2",
    "webpack": "^2.6.1",
    "webpack-node-externals": "^1.6.0"
  }
}
